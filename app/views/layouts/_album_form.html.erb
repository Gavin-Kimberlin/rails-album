<%= render "layouts/errors", :object => @album %>

<%= form_for @album do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= label_tag("Add An Artist")%>
  <%= text_field_tag :artist_name %>
  <%= f.submit %>
<% end %>

<%= link_to("Go Back To Album Details", album_path)%>
